---
title: Wish.Land - 非會員頁面
---

<!-- START from here! -->
<div class="search">
  <div class="input-group">
    <input type="text" class="form-control" name="q" value="" placeholder="搜尋其它地點...">
    <span class="input-group-btn">
      <button type="submit" class="btn btn-default"><span class="fa fa-search"></span></button>
    </span>
  </div>
</div>

<div class="qrow index">
  <!-- main function view -->
  <div class="main_map">
    <!-- map -->
    <div id="map" class="blur trans-1s"></div>
    <!-- black overlay -->
    <div class="overlay">
      <!-- detail info panel -->
      <div id="wish001" class="detail trans-1s off-canvas box-shadow">
        <!-- panelNav -->
        <div class="back">
          <a class="panelNav" href="#"><i class="fa fa-arrow-left"></i> 回到地圖</a>
        </div>

        <!-- category -->
        <div class="category trans-05s">
          <div class="icon">
            <i class="fa fa-info-circle fa-lg"></i>
          </div>
          <div class="info">
            <h4>類型</h4>
            <p>聊天喝咖啡</p>
          </div>
        </div>

        <!-- location -->
        <div class="location trans-05s">
          <div class="icon">
            <i class="fa fa-map-marker fa-lg"></i>
          </div>
          <div class="info">
            <h4>地點</h4>
            <p>台北市松山區南京東路四段 2 號</p>
          </div>
        </div>

        <!-- time -->
        <div class="time trans-05s">
          <div class="icon">
            <i class="fa fa-clock-o fa-lg"></i>
          </div>
          <div class="info">
            <h4>時間</h4>
            <p>2014-07-15 ～ 2014-07-29</p>
          </div>
        </div>

        <!-- list -->
        <div class="list trans-05s">
          <div class="icon">
            <i class="fa fa-comments-o fa-lg"></i>
          </div>
          <div class="info">
            <h4>Wish List</h4>
            <p>temp</p>
          </div>
        </div>

        <!-- host button -->
        <a href="#login" class="button trans-05s" data-toggle="modal" data-target="#myModal">
          <i class="fa fa-flag"></i> Make it happen
        </a>

      </div><!-- end of detail info panel -->

      <div id="event001" class="detail trans-1s off-canvas box-shadow">
        <!-- panelNav -->
        <div class="back">
          <a class="panelNav" href="#"><i class="fa fa-arrow-left"></i> 回到地圖</a>
        </div>

        <!-- title -->
        <div class="event-title trans-05s">
          <div class="icon">
            <i class="fa fa-ticket fa-lg"></i>
          </div>
          <div class="info">
            <h4>活動名稱</h4>
            <p>壽司吃到飽</p>
          </div>
        </div>

        <!-- category -->
        <div class="category trans-05s">
          <div class="icon">
            <i class="fa fa-info-circle fa-lg"></i>
          </div>
          <div class="info">
            <h4>類型</h4>
            <p>美食鑑賞會</p>
          </div>
        </div>

        <!-- location -->
        <div class="location trans-05s">
          <div class="icon">
            <i class="fa fa-map-marker fa-lg"></i>
          </div>
          <div class="info">
            <h4>地點</h4>
            <p>台北市松山區南京東路四段 2 號</p>
          </div>
        </div>

        <!-- time -->
        <div class="time trans-05s">
          <div class="icon">
            <i class="fa fa-clock-o fa-lg"></i>
          </div>
          <div class="info">
            <h4>時間</h4>
            <p>2014-07-15 ～ 2014-07-29</p>
          </div>
        </div>

        <!-- number -->
        <div class="number trans-05s">
          <div class="icon">
            <i class="fa fa-users fa-lg"></i>
          </div>
          <div class="info">
            <h4>目前參加人數</h4>
            <p><span>32</span> 人 / <span>60</span> 人</p>
          </div>
        </div>

        <!-- cost -->
        <div class="cost trans-05s">
          <div class="icon">
            <i class="fa fa-money fa-lg"></i>
          </div>
          <div class="info">
            <h4>參加費用</h4>
            <p>NTD <span>300</span> 元</p>
          </div>
        </div>

        <!-- host button -->
        <a href="#login" class="button trans-05s" data-toggle="modal" data-target="#myModal">
          <i class="fa fa-plus"></i> JOIN
        </a>

      </div>
    </div><!-- end of black overlay -->

    <!-- landing UI -->
    <div class="ui">
        <div class="avatar">
          <a id="discover-trigger" href="#">
            <img src="<%= lorem.image('120x120', :text => "Discover") %>" alt="" class="img-responsive img-circle box-shadow">
          </a>
        </div>
        <a href="#" class="trans-05s login-button">立即登入 <i class="fa fa-sign-in fa-lg"></i></a>
    </div><!-- end of landing UI -->

    <!-- isUser UI -->
    <div class="isUser">
      <div class="avatar">
        <a href="#wish-category">
          <img src="<%= lorem.image('120x120', :text => "Avatar") %>" alt="" class="img-responsive img-circle box-shadow">
        </a>
      </div>
      <div class="username">
        <span>FunnyQ</span>
      </div>
      <!-- fake points -->
      <a href="#" class="wish-point" data-wish="wish001"><i class="fa fa-coffee"></i></a>
      <a href="#" class="event-point" data-event="event001"><i class="fa fa-cutlery"></i></a>
    </div><!-- end of isUser UI -->

  </div><!-- end of main function view -->

</div>

<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-sm">

    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="myModalLabel">登入</h4>
      </div>
      <div class="modal-body">
        <div class="fb-login"></div>
        <div class="email-login"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">登入</button>
      </div>
    </div>

  </div>
</div>

